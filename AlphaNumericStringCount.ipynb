    "x=input(\"Enter a String\")\n",
    "digit=0\n",
    "alphabet=0\n",
    "for i in x:\n",
    "    if i.isdigit():\n",
    "        digit+=1\n",
    "    elif i.isalpha():\n",
    "        alphabet+=1\n",
    "print(\"The Number of Digits=\",digit)\n",
    "print(\"The Number of Alphabets=\",alphabet)"
   ]
